<p><strong>Programas</strong></p>
<ul class="collapsible"> 
    <% for (let programa of programas) { %>  
        <li>
            <div class="collapsible-header">
                <%= programa.nombre %>
                <span class="badge">
                    <!-- Modal Eliminar -->
                    <a class="waves-effect waves-light modal-trigger black-text" href="#EliminarPrograma"><i class="material-icons left">delete</i></a>
                </span>
            </div>
            <div class="collapsible-body">
                <div class="collection">
                    <% for (let nivel of programa.niveles) { %> 
                        <a href="./gestion-nivel-objetivos" class="collection-item"><%= nivel.nombre %></a> 
                    <% } %>
                    <a href="#" class="collection-item">Nuevo nivel</a>
                </div>
            </div>
        </li>
    <% } %>
</ul>

<!-- Modal Structure Eliminar Programa-->
<div id="EliminarPrograma" class="modal">
    <div class="modal-content">
    <h4>Eliminar usuario</h4>
    <p>¿Está seguro de eliminar este usuario?</p>
    </div>
    <div class="modal-footer">
    <a href="#" class="modal-close waves-effect waves-green btn-flat eliminarProgra">Sí</a>
    <a href="./" class="modal-close waves-effect waves-green btn-flat">Cancelar</a>
    </div>
</div><!-- Fin Modal Structure Eliminar Programa-->